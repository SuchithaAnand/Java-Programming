class Parent {
    int a, b;

    // Default constructor
    public Parent() {
        this.a = 10;
        this.b = 15;
    }

    // Parameterized constructor
    public Parent(int a, int b) {
        this.a = a;
        this.b = b;
    }

    // Method to display values of 'a' and 'b'
    public void disp() {
        System.out.println("a = " + a + ", b = " + b);
    }
}

public class Main {
    public static void main(String[] args) {
        // Creating object using default constructor
        Parent obj1 = new Parent();
        System.out.println("Values using default constructor:");
        obj1.disp(); // Calling disp() method

        // Creating object using parameterized constructor
        Parent obj2 = new Parent(20, 25);
        System.out.println("\nValues using parameterized constructor:");
        obj2.disp(); // Calling disp() method
    }
}
